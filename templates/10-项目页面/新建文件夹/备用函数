            if(sign == "zsgc"){
                window.location.href = "/xmmsr/?sign=zsgc";
            }else if(sign == "xjxm"){
                window.location.href = "/xmmsr/?sign=xjxm";
            }else if(skip.设置页面 == "zsgc"){
                window.location.href = "/xmmsr/?sign=zsgc";
            }else if(skip.设置页面 == "xjxm"){
                window.location.href = "/xmmsr/?sign=xjxm";
            }
         }
