		function skip_page(){
		    var id = ID.项目ID ;
		    var sign = get_url_param("sign");
            //if(sign == "jx"){
            //    window.location.href = "/jxxmll/";
            //}
            if(sign == "pz"){
                window.location.href = "/pzxmll/";
            }
            else if(skip.设置页面 == "zsgc"){
                id = ID.项目ID ;
                window.location.href = "/bms/?id="+String(id)+"&sign=zsgc";
            }else if(skip.设置页面 == "xjxm"){
                id = ID.项目ID ;
                window.location.href = "/bms/?id="+String(id)+"&sign=xjxm";
            }else if(sign == "zsgc"){
                id = get_url_param("id");
                window.location.href = "/bms/?id="+String(id)+"&sign=zsgc";
            }else if(sign == "xjxm"){
                id = get_url_param("id");
                window.location.href = "/bms/?id="+String(id)+"&sign=xjxm";
            }
            //else if(skip.设置页面 == "pz"){
            //    window.location.href = "/bms/?id="+String(id)+"&sign=pz";
            //}
        }
