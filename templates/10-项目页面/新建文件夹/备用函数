    //动态成表
        function dynamic_list(){
			zx_num = String(zt.执行项目数量);
            jx_num = String(zt.就绪项目数量);
            zt_num = String(zt.暂停项目数量);
            pz_num = String(zt.配置项目数量);
			
            tab="<table class=\"table table-striped table-bordered\">";
            tab+=   "<thead>";
            tab+=       "<tr>";
            tab+=           "<th>编号</th>";
            tab+=           "<th>项目状态</th>";
            tab+=           "<th>项目数量</th>";
            tab+=           "<th>查看详情</th>";
            tab+=           "<th>高级操作</th>";
            tab+=       "</tr>";
            tab+=   "</thead>";
            tab+=   "<tbody>";
            tab+=       "<tr style=\"vertical-align: middle;text-align: center;\"> ";
            tab+=           "<td>001</td>";
            tab+=           "<td>执行状态</td>";
            tab+=           "<td>"+ zx_num +"</td>";
            tab+=           "<td><button type=\"button\" class=\"btn btn-xs btn-success\" onClick=\"javascript:skip_page1()\"><span class=\"glyphicon glyphicon-zoom-in\"></span></button></b>";
            tab+=           "<td><button type=\"button\" class=\"btn btn-xs btn-primary\" onClick=\"javascript:skip_page2()\"><span class=\"glyphicon glyphicon-pawn\"></span></button></b>";
            tab+=       "</tr>";
            tab+=       "<tr style=\"vertical-align: middle;text-align: center;\">";
            tab+=           "<td>002</td>";
            tab+=           "<td>就绪状态</td>";
            tab+=           "<td>"+ jx_num +"</td>";
            tab+=           "<td><button type=\"button\" class=\"btn btn-xs btn-success\" onClick=\"javascript:skip_page3()\"><span class=\"glyphicon glyphicon-zoom-in\"></span></button></b>";
            tab+=           "<td><button type=\"button\" class=\"btn btn-xs btn-primary\" onClick=\"javascript:skip_page4()\"><span class=\"glyphicon glyphicon-pawn\"></span></button></b>";
            tab+=       "</tr>";
            tab+=       "<tr style=\"vertical-align: middle;text-align: center;\">";
            tab+=           "<td>003</td>";
            tab+=           "<td>暂停状态</td>";
            tab+=           "<td>"+ zt_num +"</td>";
            tab+=           "<td><button type=\"button\" class=\"btn btn-xs btn-success\" onClick=\"javascript:skip_page5()\"><span class=\"glyphicon glyphicon-zoom-in\"></span></button></b>";
            tab+=           "<td><button type=\"button\" class=\"btn btn-xs btn-primary\" onClick=\"javascript:skip_page6()\"><span class=\"glyphicon glyphicon-pawn\"></span></button></b>";
            tab+=       "</tr>";
            tab+=       "<tr style=\"vertical-align: middle;text-align: center;\">";
            tab+=           "<td>004</td>";
            tab+=           "<td>配置状态</td>";
