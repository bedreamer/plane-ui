     //根据进入入口不同做区分
        function display_prompt_message1() {
            var sign = get_url_param("sign");
            if((sign == "zsgc")||(sign == "xjxm")){
                var tab="<div class=\"hidden col-xs-6\" id=\"tj_content\">";
                tab+="<div class=\"input-group\">";
                tab+="<span class=\"input-group-addon\">进入区分标志:</span>";
                if(sign == "zsgc"){
                     tab+="<input type=\"text\" name=\"bz_name\" value=\"zsgc\" class=\"form-control\" placeholder=\"\">";
                }else{
                    tab+="<input type=\"text\" name=\"bz_name\" value=\"xjxm\" class=\"form-control\" placeholder=\"\">";
                }
                tab+="</div>";
                tab+="</div>";
                tishi11.innerHTML=tab;

                var id = get_url_param("id");
                var tab1="<div class=\"input-group\">";
                tab1+="<span class=\"input-group-addon\">项目ID</span>";
                tab1+="<input type=\"text\" class=\"form-control\" readonly=\"readonly\" name=\"xm_id\" value="+ id +">";
                tab1+="</div>";
                rw_id.innerHTML=tab1;

                var tab2="<div class=\"input-group\">";
                tab2+="<span class=\"input-group-addon\">项目ID</span>";
                tab2+="<input type=\"text\" class=\"form-control\" readonly=\"readonly\" name=\"xm_id\" value="+ id +">";
                tab2+="</div>";
                rw_id.innerHTML=tab2;
            }else if(skip.设置页面 == "zsgc"){
                var tab="<div class=\"hidden col-xs-6\" id=\"tj_content\">";
                tab+="<div class=\"input-group\">";
                tab+="<span class=\"input-group-addon\">进入区分标志:</span>";
                tab+="<input type=\"text\" name=\"bz_name\" value=\"zsgc\" class=\"form-control\" placeholder=\"\">";
                tab+="</div>";
                tab+="</div>";
                tishi11.innerHTML=tab;

                var tab1="<div class=\"input-group\">";
                tab1+="<span class=\"input-group-addon\">项目ID</span>";
                tab1+="<input type=\"text\" class=\"form-control\" readonly=\"readonly\" name=\"xm_id\" value="+ {{ ID.项目ID }}+">";
                tab1+="</div>";
                rw_id.innerHTML=tab1;

                var tab2="<div class=\"input-group\">";
                tab2+="<span class=\"input-group-addon\">项目ID</span>";
                tab2+="<input type=\"text\" class=\"form-control\" readonly=\"readonly\" name=\"xm_id\" value="+ {{ ID.项目ID }}+">";
                tab2+="</div>";
                rw_id1.innerHTML=tab2;
            }else if(skip.设置页面 == "xjxm"){
                var tab="<div class=\"hidden col-xs-6\" id=\"tj_content\">";
                tab+="<div class=\"input-group\">";
                tab+="<span class=\"input-group-addon\">进入区分标志:</span>";
                tab+="<input type=\"text\" name=\"bz_name\" value=\"xjxm\" class=\"form-control\" placeholder=\"\">";
                tab+="</div>";
                tab+="</div>";
                tishi11.innerHTML=tab;

                var tab1="<div class=\"input-group\">";
                tab1+="<span class=\"input-group-addon\">项目ID</span>";
                tab1+="<input type=\"text\" class=\"form-control\" readonly=\"readonly\" name=\"xm_id\" value="+ {{ ID.项目ID }} +">";
                tab1+="</div>";
                rw_id.innerHTML=tab1;

                var tab2="<div class=\"input-group\">";
                tab2+="<span class=\"input-group-addon\">项目ID</span>";
                tab2+="<input type=\"text\" class=\"form-control\" readonly=\"readonly\" name=\"xm_id\" value="+ {{ ID.项目ID }} +">";
                tab2+="</div>";
                rw_id1.innerHTML=tab2;
            }
        }
