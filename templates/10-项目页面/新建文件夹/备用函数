            tab+=           "<td><button type=\"button\" class=\"btn btn-xs btn-primary\" onClick=\"javascript:skip_page8()\"><span class=\"glyphicon glyphicon-pawn\"></span></button></b>";
            tab+=       "</tr>";
            tab+=   "</tbody>";
            tab+="</table>";
			tishi.innerHTML=tab;
		}
        
